generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Contract {
  id          Int     @id @default(autoincrement())
  link        String  @unique
  companyName String
  startDate   DateTime
  endDate     DateTime
  pay         Int
  payDate     Int
  payCycle    String
}
